<Window x:Class="session4_projects.WrapPanelExample2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:session4_projects"
        mc:Ignorable="d"
        Title="WrapPanelExample2" Height="800" Width="500">
    <WrapPanel Orientation="Vertical">
        <Ellipse Width="100" Height="100" Fill="Red"/>
        <Ellipse Width="90" Height="90" Fill="Orange"/>
        <Ellipse Width="80" Height="80" Fill="Yellow"/>
        <Ellipse Width="70" Height="70" Fill="Green"/>
        <Ellipse Width="60" Height="60" Fill="Blue"/>
        <Ellipse Width="50" Height="50" Fill="Indigo"/>
        <Ellipse Width="40" Height="40" Fill="Purple"/>
    </WrapPanel>
</Window>
