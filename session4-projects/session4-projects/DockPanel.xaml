<Window x:Class="session4_projects.DockPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:session4_projects"
        mc:Ignorable="d"
        Title="DockPanel" Height="450" Width="800">
    <DockPanel Margin="15" Background="LightCoral">
        <Button Content="Left" DockPanel.Dock="Left" Background="Wheat" FontSize="20"></Button>
        <Button Content="Right" DockPanel.Dock="Right" Background="Yellow" FontSize="20"/>
        <Button Content="Top" DockPanel.Dock="Top" Background="Wheat" FontSize="20"/>
        <Button Content="Bottom" DockPanel.Dock="Bottom" Background="Yellow" FontSize="20"/>

        <Button Content="Fill" FontSize="20" Background="Red"/>
    </DockPanel>
</Window>
